    <div class="body_box_outer">
        <div class="body_box_inner">

          <div class="text_body_heading">
          click on a todo item to view details, change or delete
          </div>

          <div class="text_body_todo">
             <% todos.each do |todo| %>
            <%= render partial: 'todo', locals: { todo: todo } %>
            <form action="/todos" method="POST">
          <p>Task: <input type="text" name="Task"></p>
          <p>Complete: <input type="checkbox" name="Complete"></p>
          <input type="submit" value-"CREATE NEW TODO">
          </form>
            <% end %>
          </div>
        </div>
      </div>
    </div>

